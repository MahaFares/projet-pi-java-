<form method="post" action="{{ path('app_chambre_delete', {'id': chambre.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette chambre ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ chambre.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
