{% extends 'base.html.twig' %}

{% block title %}Transport{% endblock %}

{% block body %}
    <h1>Transport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transport.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ transport.type }}</td>
            </tr>
            <tr>
                <th>Capacite</th>
                <td>{{ transport.capacite }}</td>
            </tr>
            <tr>
                <th>Emissionco2</th>
                <td>{{ transport.emissionco2 }}</td>
            </tr>
            <tr>
                <th>Prixparpersonne</th>
                <td>{{ transport.prixparpersonne }}</td>
            </tr>
            <tr>
                <th>Disponible</th>
                <td>{{ transport.disponible ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_transport_index') }}">back to list</a>

    <a href="{{ path('app_transport_edit', {'id': transport.id}) }}">edit</a>

    {{ include('transport/_delete_form.html.twig') }}
{% endblock %}
